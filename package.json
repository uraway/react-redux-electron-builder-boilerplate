{
  "main": "main.js",
  "name": "react-redux-electron-builder-boilerplate",
  "author": "Masato Urai",
  "version": "0.0.1",
  "description": "Electron application",
  "scripts": {
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "cross-env NODE_ENV=production npm run webpack -- --config ./webpack.electron.config",
    "build-renderer": "cross-env NODE_ENV=production npm run webpack -- --config ./webpack.production.config",
    "build-all": "npm run build && npm run prepare",
    "lint": "eslint src test *.js",
    "package": "npm run prepare && npm run build && build --osx",
    "version": "node -r babel-register ./scripts/version.js",
    "prepare": "node -r babel-register ./scripts/prepare.js",
    "start": "concurrently --kill-others \"npm run start-server\" \"npm run start-hot\"",
    "start-server": "node -r babel-register server.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-watch": "npm test -- --watch",
    "webpack": "node -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors"
  },
  "dependencies": {
    "react": "15.2.0",
    "react-addons-pure-render-mixin": "15.2.0",
    "react-dom": "15.2.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-resolver": "0.0.7",
    "babel-plugin-webpack-loaders": "0.7.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "concurrently": "2.2.0",
    "cross-env": "^1.0.8",
    "css-loader": "0.23.1",
    "devtron": "1.2.1",
    "electron-builder": "5.10.5",
    "electron-debug": "1.0.1",
    "electron-mocha": "2.2.1",
    "electron-prebuilt": "1.2.6",
    "enzyme": "2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fs-extra": "0.30.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "react-addons-test-utils": "15.2.0",
    "react-hot-loader": "3.0.0-beta.2",
    "sass-lint": "1.8.2",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1"
  },
  "build": {
    "appId": "com.yourname.electron"
  },
  "directories": {
    "output": "release"
  }
}
